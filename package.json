{
  "name": "expressproject",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "model-generation": "npx sequelize-cli model:generate --name User --attributes firstName:string,lastName:string,email:string",
    "migrate": "npx sequelize-cli db:migrate",
    "drop": "npx sequelize-cli db:migrate:undo:all",
    "start": "node ./bin/www",
    "createDb": "node -e 'require(\"./utils/database.js\").createDb()'",
    "dropDb": "node -e 'require(\"./utils/database.js\").dropDb()'"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "^0.29.0",
    "morgan": "~1.9.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "pgtools": "^0.1.1",
    "sequelize": "^7.0.0-alpha.2"
  }
}
